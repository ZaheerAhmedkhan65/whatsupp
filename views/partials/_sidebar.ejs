<div class="sidebar">
    <h2>Users</h2>
    <ul class="user-list">
      <% users.forEach(user => { %>
        <li>
          <a href="/messages/chat?token=<%= token %>&receiverId=<%= user.id %>">
            <%= user.username %>
          </a>
        </li>
      <% }) %>
      <li class="logout"><a href="/auth/logout">Logout</a></li>
    </ul>
</div>